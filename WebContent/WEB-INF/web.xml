<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>FirstServletProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>XmlServletName</servlet-name>
  	<servlet-class>com.saral.training.servlet1.XmlServlet</servlet-class>
  	<init-param>
  		<param-name>defaultkaam</param-name>
  		<param-value>saxena with xml</param-value>
  	</init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>XmlServletName</servlet-name>
	<url-pattern>/firstXMLservlet</url-pattern>
  </servlet-mapping>
  
  
  
  
</web-app>